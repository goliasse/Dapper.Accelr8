        </dependencies>
        <frameworkAssemblies>
            <frameworkAssembly assemblyName="Microsoft.CSharp" targetFramework=".NETFramework4.0" />
        </frameworkAssemblies>
    </metadata>
    <files>
        <file src="..\Dapper.Accelr8.Repo\bin\Release\Dapper.Accelr8.Repo.dll" target="lib\net40\Dapper.Accelr8.Repo.dll" />
